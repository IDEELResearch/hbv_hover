/* BARUCH CSS */
.library-container{
margin: 0 auto;
text-align: left;
min-width: 999px;
}

#library-topbar {
min-width: 994px;
height: 36px;
background: #000000 url("https://www.baruch.cuny.edu/library_assets/global/images/topbarbg.png") 0 0 repeat-x;
color: #fbff96;
}

#library-topbar img {
vertical-align: middle;
}

#tb-container {
height: 36px;
padding-top: 6px;
}

#library-topbar ul {
list-style-type: none;
margin:0;
padding:0;
}

#library-topbar li {
float: right;
font: normal bold 12px/16px Helvetica, Arial, sans-serif;
border-right: 1px solid #999;
height: 15px;
}

#library-topbar a:link, #library-topbar a:visited {
color: #f4c430;
text-decoration: none;
}

#library-topbar a:hover, #library-topbar a:focus {
color: #fbff96;
text-decoration: none;
}

#library-toplinks {
padding-top:3px;
}

#library-toplinks a {
padding: 0 12px;
}

#library-header {
background: #0f1722 url("https://www.baruch.cuny.edu/library_assets/global/images/h-containerbg.png") center 0 repeat-x;
height: 113px;
border-bottom: 1px #191919 solid;
overflow: hidden;
}

#h-container {
height: 113px;
min-width: 999px;
background: transparent url("https://www.baruch.cuny.edu/library_assets/global/images/headerbg.jpg") center 0 no-repeat;
}

.library-space-container {
width: 950px;
margin: 0 auto;
}

#newmanlogo a {
font: 200% Helvetica, Arial, sans-serif;
color: #FBFF96;
text-decoration: none;
text-indent: -9999px;
position: absolute;
width: 440px;
height: 60px;
top: 60px;
}

#library-navigation {
min-width: 994px;
background: #0b1726 url("https://www.baruch.cuny.edu/library_assets/global/images/headerbg.jpg") center bottom no-repeat;
font: normal bold 14px/26px Helvetica, Arial, sans-serif;
font: normal 14px/26px Helvetica, Arial, sans-serif;
border-top: 1px #ccc solid;
overflow: hidden;
}

#library-globalnav {
 margin:0;
 padding:0;
}

#library-globalnav li {
float: left;
height: 36px;
list-style-type:none;
}

/* Top-Level Text */
#library-globalnav a, #library-globalnav a:link, #library-globalnav a:visited {
    background-color:transparent;
    color:#ffffff;
    text-decoration:none;
    display:block;
    float:left;
    padding:5px 0px 5px 6px;
    margin-left:5px;
}
#library-globalnav a+span {float:left;display:block;width:6px;height:36px;}
#library-globalnav a:hover {color:#225391;background:transparent url(http://www.baruch.cuny.edu/library_assets/global/images/navpill.png) left -36px no-repeat;}
#library-globalnav a:hover+span {background:transparent url(http://www.baruch.cuny.edu/library_assets/global/images/navpill.png) right -36px no-repeat;}
#library-globalnav a.dd:hover,#library-globalnav a.dd:hover+span {background-position:0 -72px;}

/* Top-Level BG after cursor moves away */
#library-globalnav li:hover {
    background-color:transparent;
}

/* Top-Level Links after cursor moves away */
#library-globalnav li:hover a.dd,
#students   #library-globalnav li:hover a.dd,
#alumni     #library-globalnav li:hover a.dd,
#guests     #library-globalnav li:hover a.dd,
#help       #library-globalnav li:hover a.dd,
#news       #library-globalnav li:hover a.dd,
#about      #library-globalnav li:hover a.dd {color:#225391;background:transparent url(http://www.baruch.cuny.edu/library_assets/global/images/navpill.png) left -72px no-repeat;}
#library-globalnav li:hover a.dd+span,
#students   #library-globalnav li:hover a.dd+span,
#alumni     #library-globalnav li:hover a.dd+span,
#guests     #library-globalnav li:hover a.dd+span,
#help       #library-globalnav li:hover a.dd+span,
#news       #library-globalnav li:hover a.dd+span,
#about      #library-globalnav li:hover a.dd+span {background:transparent url(http://www.baruch.cuny.edu/library_assets/global/images/navpill.png) right -72px no-repeat;}

#library-footer {
min-width: 994px;
margin: 0px auto;
background: #000000 url("https://www.baruch.cuny.edu/library_assets/global/images/footerbg.jpg") center 0 no-repeat;
border-top: 12px solid #225391;
text-align: left;
font-size: 11px;
}

#library-footer p {
color: rgb(200,200,200);
font-size: 11px;
}

#f-container {
height: 200px;
padding: 24px 22px;
}

#f-logos {
text-align: right;
}

#f-logos img {
margin-bottom: 9px;
}

#f-address p {
line-height: 19px;
padding: 3px 0 0 5px;
margin: 0 0 10px 0;
}

#f-credits {
padding-left: 57px;
}

#f-links {
margin-top:0;
}

#f-links a {
font-size: 11px;
text-decoration: underline;
font-weight: bold;
color: #5c94cd;
}

#f-links li {
padding-bottom: 5px;
}

.colborder {
padding-right: 24px;
margin-right: 25px;
border-right: 1px solid #ddd;
}

#header {background-color:#000;}
#header .cell {margin: 0 auto; width: 930px;}
